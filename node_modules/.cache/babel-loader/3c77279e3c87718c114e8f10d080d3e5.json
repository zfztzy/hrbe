CA;AAAAD;AAAAC;AAAA,OADA,EAEA;AAAAD;AAAAC;AAAA,OAFA;AAIA;AACAkB,0CADA;AAEAC,6CAFA;AAGAC,sCAHA;AAIAC,4CAJA;AAKAC,sCALA;AAMAC;AANA;AAQA;AACA,KA5CA;AA6CAC,gBA7CA,0BA6CA;AACA;AACAC,qDADA;AAEAC,wDAFA;AAGAC,iDAHA;AAIAC,uDAJA;AAKAC,iDALA;AAMAC;AANA;AAQAC;AACA,KAvDA;AAwDAC,OAxDA,eAwDAC,CAxDA,EAwDA;AACAF;AACA;AA1DA,GArBA;AAiFAG;AACA;AADA,GAjFA;AAoFAC,SApFA,qBAoFA;AACA;AACA;AAtFA","names":["name","data","filter","vModel","title","filter2","newSwitch","BatchNum","isShow","filterData","cleanNum","methods","profileDetail","type","path","refresh","window","batchInput","batchOutput","newInfo","switchTable","resetFilter","recommender","recommend_time","related","resume_status","region","job","updateFilter","recommender__icontains","recommend_time__icontains","related__icontains","resume_status__icontains","region__icontains","job__icontains","console","log","a","watch","mounted"],"sourceRoot":"src/views/hr_manage_table","sources":["HrManageTable.vue"],"sourcesContent":["<template>\n  <a-layout-content>\n    <a-space :size=\"8\" style=\"width: 100%; margin: 20px\" class=\"\" v-if=\"isShow\">\n      <div v-for='(i) in filter' :key=\"i.title\"  style=\"width:300px\"><a-input :placeholder=\"i.title\" v-model=\"i.vModel\" type=\"text\"/></div>\n    </a-space>\n    <a-space :size=\"8\" style=\"width: 100%; margin-left: 20px; margin-bottom: 20px; margin-right: 20px\" class=\"\" v-if=\"isShow\">\n      <div v-for='(i) in filter2' :key=\"i.title\"  style=\"width:300px\"><a-input :placeholder=\"i.title\" v-model=\"i.vModel\" type=\"text\"/></div>\n        <a-button @click=\"updateFilter\">搜索</a-button>\n        <a-button @click=\"resetFilter\">清除</a-button>\n    </a-space>\n    <a-space style=\"float:right\">\n      <a-button class=\"tableButton\" @click=\"newInfo\">新增</a-button>\n      <a-button class=\"tableButton\" @click=\"batchInput\">批量导入</a-button>\n      <a-button class=\"tableButton\" @click=\"batchOutput\">批量导出</a-button>\n    </a-space>\n    <router-view :BatchNum=\"BatchNum\" :newSwitch=\"newSwitch\" :filterData='filterData' :cleanNum=\"cleanNum\"/>\n  </a-layout-content>\n</template>\n\n<script>\n// @ is an alias to /src\n\n\n\nexport default {\n  name: 'Profile',\n  data(){\n    return{\n      filter:[\n        {vModel: undefined, title: '招聘顾问'},\n        {vModel: undefined, title: '候选人推荐时间'},\n        {vModel: undefined, title: '关联需求'},\n        {vModel: undefined, title: '简历状态'},\n      ],\n      filter2: [\n        {vModel: undefined, title: '地域'},\n        {vModel: undefined, title: '岗位'}\n      ],\n      newSwitch: 0,\n      BatchNum: 0,\n      isShow: false,\n      filterData: {},\n      cleanNum: 0\n    }\n  },    \n  methods:{\n    profileDetail(type){\n        this.$router.push({path:'/hrManageTable/' + type})\n    },\n    refresh(){\n      window.scrollTo(0,0);\n      this.switchTable()\n    },\n    batchInput () {\n      this.BatchNum += 1\n    },\n    batchOutput () {\n      this.BatchNum -= 1\n    },\n    newInfo() {\n      this.newSwitch += 1\n    },\n    switchTable () {\n      if (this.$route.name==='Applicant') {\n        this.isShow = true\n      } else {\n        this.isShow = false\n      }\n    },\n    resetFilter () {\n      this.filter = [\n        {vModel: undefined, title: '招聘顾问'},\n        {vModel: undefined, title: '候选人推荐时间'},\n        {vModel: undefined, title: '关联需求'},\n        {vModel: undefined, title: '简历状态'}\n      ]\n      this.filter2 = [\n        {vModel: undefined, title: '地域'},\n        {vModel: undefined, title: '岗位'}\n      ]\n      this.filterData = {\n        recommender: this.filter[0].vModel,\n        recommend_time: this.filter[1].vModel,\n        related: this.filter[2].vModel,\n        resume_status: this.filter[3].vModel,\n        region: this.filter2[0].vModel,\n        job: this.filter2[1].vModel\n      }\n      this.cleanNum += 1\n    },\n    updateFilter () {\n      this.filterData = {\n        recommender__icontains: this.filter[0].vModel,\n        recommend_time__icontains: this.filter[1].vModel,\n        related__icontains: this.filter[2].vModel,\n        resume_status__icontains: this.filter[3].vModel,\n        region__icontains: this.filter2[0].vModel,\n        job__icontains: this.filter2[1].vModel\n      }\n      console.log(this.filterData);\n    },\n    log (a) {\n      console.log(a);\n    }\n  },\n  watch:{\n    '$route': 'refresh'\n  },\n  mounted () {\n    this.switchTable()\n  }\n}\n\n</script>\n\n<style>\n.tableButton {\n  float:right;\n  margin: 20px;\n  z-index: 100;\n}\n</style>"]}]}