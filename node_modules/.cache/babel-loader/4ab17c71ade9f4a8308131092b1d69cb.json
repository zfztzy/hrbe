(obj)\" @click=\"imgClick(obj, item.num)\" :src='item.src' alt=\"\" style=\"height: 321px;width: 514px;\">\n        <h2 v-show=\"item.isShow\">⬆️这里是关于{{item.text}}</h2>\n        <h3 v-show=\"item.isShow\">{{item.postscript}}</h3>\n      </template>  \n    </slide>\n    \n  </carousel-3d>\n</template>\n\n\n<script>\nimport { Carousel3d, Slide } from 'vue-carousel-3d'\nexport default {\n    name:'Homebanner',\n    components: {\n        Carousel3d,\n        Slide\n    },\n    data(){\n        return{\n            slides:[\n                {text:'我自己',num:0,src:require('../../assets/homebanner/me.png'),postscript:'想了解我这个人，点这里',isShow:false},\n                {text:'这个网页',num:1,src:require('../../assets/homebanner/page.jpeg'),postscript:'想知道这个网页怎么来的，点这里',isShow:false},\n                {text:'工作',num:2,src:require('../../assets/homebanner/work.jpg'),postscript:'想知道我能干嘛，点这里',isShow:false},\n                {text:'技能',num:3,src:require('../../assets/homebanner/skill.jpeg'),postscript:'想知道我喜欢什么，点这里',isShow:false},\n                {text:'FG',num:0,src:require('../../assets/homebanner/me.png'),postscript:'想了解我这个人，点这里',isShow:false},\n                {text:'这个网页',num:1,src:require('../../assets/homebanner/page.jpeg'),postscript:'想知道这个网页怎么来的，点这里',isShow:false},\n                {text:'工作',num:2,src:require('../../assets/homebanner/work.jpg'),postscript:'想知道我能干嘛，点这里',isShow:false},\n                {text:'技能',num:3,src:require('../../assets/homebanner/skill.jpeg'),postscript:'想知道我喜欢什么，点这里',isShow:false},\n                ],\n            pathList:['/profile/partprofile/lifeProfile','/profile/partprofile/page','/profile/partprofile/work','/profile/partprofile/skill']\n        }\n    },\n    computed:{\n        \n    },\n    methods:{\n        imgClick(i,a){\n            if(i.leftIndex==-1&i.rightIndex==-1){\n                this.$router.push({\n                path:this.pathList[a],\n                query:{\n                    id:a    \n                }\n                })\n            }\n        },\n        show(i){\n            let a = i.index\n            if(a<3){\n                this.slides[a].isShow = true\n            }else{\n                this.slides[a].isShow = true\n            }\n\n        },\n        disshow(i){\n            let a = i.index\n            if(a<3){\n                this.slides[a].isShow = false\n            }else{\n                this.slides[a].isShow = false\n            }\n\n        }\n    }\n}\n</script>\n\n<style>\n.carousel-3d-slide {\n  background: none;\n  border: 0px;\n  text-align:center;\n  \n}\n</style>"],"sourceRoot":"src/components/banner"}]}