KAzCA;AA0CAC,gBA1CA,0BA0CA;AACA;AACAC,qDADA;AAEAC,wDAFA;AAGAC,iDAHA;AAIAC,uDAJA;AAKAC,gDALA;AAMAC;AANA;AAQAC;AACA,KApDA;AAqDAC,OArDA,eAqDAC,CArDA,EAqDA;AACAF;AACA;AAvDA,GAzBA;AAkFAG;AACA;AADA,GAlFA;AAqFAC,SArFA,qBAqFA;AACA;AACA;AAvFA","names":["components","name","data","links","type","msg","filter","vModel","title","newSwitch","BatchNum","isShow","filterData","methods","profileDetail","path","refresh","window","batchInput","batchOutput","newInfo","switchTable","resetFilter","recommender","recommend_time","related","resume_status","region","job","updateFilter","recommender__icontains","recommend_time__icontains","related__icontains","resume_status__icontains","region__icontains","job__icontains","console","log","a","watch","mounted"],"sourceRoot":"src/views/hr_manage_table","sources":["HrManageTable.vue"],"sourcesContent":["<template>\n  <div class=\"profile\">\n    <router-view :BatchNum=\"BatchNum\" :newSwitch=\"newSwitch\" :filterData='filterData'/>\n  </div>\n</template>\n\n<script>\n// @ is an alias to /src\n\n\n\nexport default {\n  components: {  },\n  name: 'Profile',\n  data(){\n    return{\n      links:[\n            {type:'applicant', msg: '候选人管理', },\n            {type:'recruitment', msg: '招聘需求', },\n            {type:'pojectStatus', msg: '项目满足度', },\n            {type:'projectInfo', msg: '项目信息', },\n      ],\n      filter:[\n        {vModel: undefined, title: '招聘顾问'},\n        {vModel: undefined, title: '候选人推荐时间'},\n        {vModel: undefined, title: '关联需求'},\n        {vModel: undefined, title: '简历状态'},\n        {vModel: undefined, title: '地域'},\n        {vModel: undefined, title: '岗位'},\n      ],\n      newSwitch: 0,\n      BatchNum: 0,\n      isShow: false,\n      filterData: {}\n    }\n  },    \n  methods:{\n    profileDetail(type){\n        this.$router.push({path:'/hrManageTable/' + type})\n    },\n    refresh(){\n      window.scrollTo(0,0);\n      this.switchTable()\n    },\n    batchInput () {\n      this.BatchNum += 1\n    },\n    batchOutput () {\n      this.BatchNum -= 1\n    },\n    newInfo() {\n      this.newSwitch += 1\n    },\n    switchTable () {\n      if (this.$route.name==='Applicant') {\n        this.isShow = true\n      } else {\n        this.isShow = false\n      }\n    },\n    resetFilter () {\n      this.filter = [\n        {vModel: undefined, title: '招聘顾问'},\n        {vModel: undefined, title: '候选人推荐时间'},\n        {vModel: undefined, title: '关联需求'},\n        {vModel: undefined, title: '简历状态'},\n        {vModel: undefined, title: '地域'},\n        {vModel: undefined, title: '岗位'},\n      ]\n      this.filterData = {\n        recommender: this.filter[0].vModel,\n        recommend_time: this.filter[1].vModel,\n        related: this.filter[2].vModel,\n        resume_status: this.filter[3].vModel,\n        region: this.filter[4].vModel,\n        job: this.filter[5].vModel\n      }\n    },\n    updateFilter () {\n      this.filterData = {\n        recommender__icontains: this.filter[0].vModel,\n        recommend_time__icontains: this.filter[1].vModel,\n        related__icontains: this.filter[2].vModel,\n        resume_status__icontains: this.filter[3].vModel,\n        region__icontains: this.filter[4].vModel,\n        job__icontains: this.filter[5].vModel\n      }\n      console.log(this.filterData);\n    },\n    log (a) {\n      console.log(a);\n    }\n  },\n  watch:{\n    '$route': 'refresh'\n  },\n  mounted () {\n    this.switchTable()\n  }\n}\n\n</script>\n"]}]}