 slides\" :index=\"i\" :key=\"i\">{{item.text}}\n        <!--通过插槽作用域可以拿到点击的图片的索引-->\n      <template slot-scope=\"obj\">\n        <img @mouseover=\"show(obj)\" @mouseleave=\"disshow(obj)\" @click=\"imgClick(obj, item.type)\" :src='item.src' alt=\"\" style=\"height: 321px;width: 514px; border-radius: 19px\">\n        <h2 v-show=\"item.isShow\">⬆️这里是关于{{item.text}}</h2>\n        <h3 v-show=\"item.isShow\">{{item.postscript}}</h3>\n      </template>  \n    </slide>\n    \n  </carousel-3d>\n</template>\n\n\n<script>\nimport { Carousel3d, Slide } from 'vue-carousel-3d'\nexport default {\n    name:'Homebanner',\n    components: {\n        Carousel3d,\n        Slide\n    },\n    data(){\n        return{\n            slides:[\n                {text:'我自己',type:'me',src:require('../../assets/homebanner/me1.png'),postscript:'想了解我这个人，点这里',isShow:false},\n                {text:'这个网页',type:'page',src:require('../../assets/homebanner/page1.png'),postscript:'想知道这个网页怎么来的，点这里',isShow:false},\n                {text:'工作',type:'work',src:require('../../assets/homebanner/work1.png'),postscript:'想知道我能干嘛，点这里',isShow:false},\n                {text:'技能',type:'skill',src:require('../../assets/homebanner/skill1.png'),postscript:'想知道我喜欢什么，点这里',isShow:false},\n                {text:'我自己',type:'me',src:require('../../assets/homebanner/me1.png'),postscript:'想了解我这个人，点这里',isShow:false},\n                {text:'这个网页',type:'page',src:require('../../assets/homebanner/page1.png'),postscript:'想知道这个网页怎么来的，点这里',isShow:false},\n                {text:'工作',type:'work',src:require('../../assets/homebanner/work1.png'),postscript:'想知道我能干嘛，点这里',isShow:false},\n                {text:'技能',type:'skill',src:require('../../assets/homebanner/skill1.png'),postscript:'想知道我喜欢什么，点这里',isShow:false},\n                ],\n        }\n    },\n    computed:{\n        \n    },\n    methods:{\n        imgClick(i,type){\n            if(i.leftIndex==-1&i.rightIndex==-1){\n                this.$router.push({path:'/profile/partprofile',query: {type:type}})   \n            }\n        },\n        show(i){\n            let a = i.index\n            if(a<3){\n                this.slides[a].isShow = true\n            }else{\n                this.slides[a].isShow = true\n            }\n\n        },\n        disshow(i){\n            let a = i.index\n            if(a<3){\n                this.slides[a].isShow = false\n            }else{\n                this.slides[a].isShow = false\n            }\n\n        }\n    }\n}\n</script>\n"],"sourceRoot":"src/components/banner"}]}