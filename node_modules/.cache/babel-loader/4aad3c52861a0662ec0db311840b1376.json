ADA;AAOAC,gBACA;AAAAF;AAAAC;AAAA,OADA,EAEA;AAAAD;AAAAC;AAAA,OAFA,CAPA;AAWAE,kBAXA;AAYAC,iBAZA;AAaAC,mBAbA;AAcAC,oBAdA;AAeAC,iBAfA;AAgBAC,4BAhBA;AAiBAC,oBAjBA;AAkBAC;AAlBA;AAoBA,GAvBA;AAwBAC;AACAC,iBADA,yBACAC,IADA,EACA;AACA;AAAAC;AAAA;AACA,KAHA;AAIAC,cAJA,wBAIA;AACA;AACA,KANA;AAOAC,gBAPA,0BAOA;AACA;AACA,KATA;AAUAC,aAVA,qBAUAC,IAVA,EAUAC,UAVA,EAUA;AACA;AACAC;AACA,KAbA;AAcAC,WAdA,qBAcA;AACAC;AACA;AACA,KAjBA;AAkBAC,cAlBA,wBAkBA;AACA;AACA,KApBA;AAqBAC,eArBA,yBAqBA;AACA;AACA,KAvBA;AAwBAC,WAxBA,qBAwBA;AACA;AACA,KA1BA;AA2BAC,eA3BA,yBA2BA;AACA;AACA;AACA,OAFA,MAEA;AACA;AACA;;AACA;AACA;AACA,OAFA,MAEA;AACA;AACA;AACA,KAtCA;AAuCAC,eAvCA,yBAuCA;AACA,qBACA;AAAA3B;AAAAC;AAAA,OADA,EAEA;AAAAD;AAAAC;AAAA,OAFA,EAGA;AAAAD;AAAAC;AAAA,OAHA,EAIA;AAAAD;AAAAC;AAAA,OAJA;AAMA,sBACA;AAAAD;AAAAC;AAAA,OADA,EAEA;AAAAD;AAAAC;AAAA,OAFA;AAIA;AACA2B,0CADA;AAEAC,6CAFA;AAGAC,sCAHA;AAIAC,4CAJA;AAKAC,sCALA;AAMAC;AANA;AAQA;AACA,KA3DA;AA4DAC,gBA5DA,0BA4DA;AACA;AACAC,qDADA;AAEAC,oDAFA;AAGAC,iDAHA;AAIAC,uDAJA;AAKAC,iDALA;AAMAC;AANA;AAQApB;AACA,KAtEA;AAuEAqB,YAvEA,oBAuEAtB,UAvEA,EAuEA;AACA;AACA;AACAC;AACAA;AACA;AACA;AACA;AACAA;AACA,KAhFA;AAiFAsB,QAjFA,gBAiFAvB,UAjFA,EAiFA;AACA;AACA,KAnFA;AAoFAwB,OApFA,eAoFAC,UApFA,EAoFA;AACAxB;AACA;AAtFA,GAxBA;AAgHAyB;AACAC;AACAC,aADA,qBACA;AACA,2BADA,CAEA;;AACA;AACA;AALA;AADA,GAhHA;AAyHAC,SAzHA,qBAyHA;AACA,uBADA,CAEA;AACA;AACA;AACA;AACA;AA/HA","names":["o","name","data","filter","vModel","title","filter2","newSwitch","BatchNum","isShow","filterData","cleanNum","isProjectStatus","visible","selectDate","methods","profileDetail","type","path","showDrawer","visibleClose","onChange2","date","dateString","console","refresh","window","batchInput","batchOutput","newInfo","switchTable","resetFilter","recommender","recommend_time","related","resume_status","region","job","updateFilter","recommender__icontains","recommend_time__range","related__icontains","resume_status__icontains","region__icontains","job__icontains","onChange","onOk","log","childValue","watch","$route","handler","mounted"],"sourceRoot":"src/views/hr_manage_table","sources":["HrManageTable.vue"],"sourcesContent":["<template>\n  <a-layout-content>\n    <a-space :size=\"8\" style=\"width: 100%; margin-top: 20px; margin-bottom: 20px\" class=\"\" v-if=\"isShow\">\n          <!-- :show-time=\"{ format: 'HH:mm' }\" -->\n      <template  v-for='(i) in filter'>\n        <a-range-picker\n          style=\" margin-left: 20px; \"\n          v-if=\"i.title=='候选人推荐时间'\"\n          :key=\"i.title\"\n          format=\"YYYY-MM-DD\"\n          :placeholder=\"['候选人推荐时间开始', '候选人推荐时间结束']\"\n          @change=\"onChange\"\n          @ok=\"onOk\"\n        />\n        <div v-else :key=\"i.title\"  style=\"width:300px\">\n          <a-input style=\" margin-left: 20px; \" :placeholder=\"i.title\" v-model=\"i.vModel\" type=\"text\"/>\n        </div>\n      </template>\n    </a-space>\n    <a-space :size=\"8\" style=\"width: 100%;margin-bottom: 20px;\" class=\"\" v-if=\"isShow\">\n      <template v-for='(i) in filter2'>\n        <div :key=\"i.title\"  style=\"width:300px\">\n          <a-input style=\" margin-left: 20px; \" :placeholder=\"i.title\" v-model=\"i.vModel\" type=\"text\"/>\n        </div>\n      </template>\n        <a-button style=\" margin-left: 20px; \" @click=\"updateFilter\">搜索</a-button>\n        <a-button style=\" margin-left: 20px; \" @click=\"resetFilter\">清除</a-button>\n    </a-space>\n    <a-space v-if=\"!isProjectStatus\" style=\"float:right\">\n      <a-button class=\"tableButton\" @click=\"newInfo\">新增</a-button>\n      <a-button class=\"tableButton\" @click=\"batchInput\">批量导入</a-button>\n      <a-button class=\"tableButton\" @click=\"batchOutput\">批量导出</a-button>\n    </a-space>\n    <a-space class=\"SelectMonth\" v-if=\"isProjectStatus\">\n      <a-month-picker placeholder=\"Select month\" @change=\"onChange2\" />\n    </a-space>\n    <a-space v-if=\"isProjectStatus\" style=\"float:right\">\n      <a-button class=\"tableButton\" @click=\"newInfo\">新增</a-button>\n      <a-button class=\"tableButton\" @click=\"batchOutput\">导出</a-button>\n      <a-button class=\"tableButton\" @click=\"showDrawer\">图表总览</a-button>\n    </a-space>\n    <router-view \n      :visible='visible' \n      @visibleClose='visibleClose'\n      :selectDate='selectDate'\n      tableType='test' \n      @save='log' \n      :BatchNum=\"BatchNum\" \n      :newSwitch=\"newSwitch\" \n      :filterData='filterData' \n      :cleanNum=\"cleanNum\"/>\n  </a-layout-content>\n</template>\n\n<script>\n// @ is an alias to /src\n\n\n// deepCopy\nfunction typeOf(obj) {\n  const toString = Object.prototype.toString;\n  const map = {\n    '[object Boolean]'  : 'boolean',\n    '[object Number]'   : 'number',\n    '[object String]'   : 'string',\n    '[object Function]' : 'function',\n    '[object Array]'    : 'array',\n    '[object Date]'     : 'date',\n    '[object RegExp]'   : 'regExp',\n    '[object Undefined]': 'undefined',\n    '[object Null]'     : 'null',\n    '[object Object]'   : 'object'\n  };\n  return map[toString.call(obj)];\n}\nfunction deepCopy(data) {\n  const t = typeOf(data);\n  let o;\n\n  if (t === 'array') {\n    o = [];\n  } else if ( t === 'object') {\n    o = {};\n  } else {\n    return data;\n  }\n\n  if (t === 'array') {\n    for (let i = 0; i < data.length; i++) {\n      o.push(deepCopy(data[i]));\n    }\n  } else if ( t === 'object') {\n    for (let i in data) {\n      o[i] = deepCopy(data[i]);\n    }\n  }\n  return o;\n}\n\n\nexport default {\n  name: 'Profile',\n  data(){\n    return{\n      filter:[\n        {vModel: undefined, title: '招聘顾问'},\n        {vModel: undefined, title: '候选人推荐时间'},\n        {vModel: undefined, title: '关联需求'},\n        {vModel: undefined, title: '简历状态'},\n      ],\n      filter2: [\n        {vModel: undefined, title: '地域'},\n        {vModel: undefined, title: '岗位'}\n      ],\n      newSwitch: 0,\n      BatchNum: 0,\n      isShow: false,\n      filterData: {},\n      cleanNum: 0,\n      isProjectStatus: false,\n      visible: false,\n      selectDate: undefined\n    }\n  },    \n  methods:{\n    profileDetail(type){\n        this.$router.push({path:'/hrManageTable/' + type})\n    },\n    showDrawer() {\n      this.visible = true;\n    },\n    visibleClose () {\n      this.visible = false;\n    },\n    onChange2(date, dateString) {\n      this.selectDate = dateString.substring(0,4) + dateString.substring(5,7)\n      console.log(this.selectDate);\n    },\n    refresh(){\n      window.scrollTo(0,0);\n      this.switchTable()\n    },\n    batchInput () {\n      this.BatchNum += 1\n    },\n    batchOutput () {\n      this.BatchNum -= 1\n    },\n    newInfo() {\n      this.newSwitch += 1\n    },\n    switchTable () {\n      if (this.$route.name==='applicant') {\n        this.isShow = true\n      } else {\n        this.isShow = false\n      }\n      if (this.$route.name==='pojectstatus') {\n        this.isProjectStatus = true\n      } else {\n        this.isProjectStatus = false\n      }\n    },\n    resetFilter () {\n      this.filter = [\n        {vModel: undefined, title: '招聘顾问'},\n        {vModel: undefined, title: '候选人推荐时间'},\n        {vModel: undefined, title: '关联需求'},\n        {vModel: undefined, title: '简历状态'}\n      ]\n      this.filter2 = [\n        {vModel: undefined, title: '地域'},\n        {vModel: undefined, title: '岗位'}\n      ]\n      this.filterData = {\n        recommender: this.filter[0].vModel,\n        recommend_time: this.filter[1].vModel,\n        related: this.filter[2].vModel,\n        resume_status: this.filter[3].vModel,\n        region: this.filter2[0].vModel,\n        job: this.filter2[1].vModel\n      }\n      this.cleanNum += 1\n    },\n    updateFilter () {\n      this.filterData = {\n        recommender__icontains: this.filter[0].vModel,\n        recommend_time__range: this.filter[1].vModel,\n        related__icontains: this.filter[2].vModel,\n        resume_status__icontains: this.filter[3].vModel,\n        region__icontains: this.filter2[0].vModel,\n        job__icontains: this.filter2[1].vModel\n      }\n      console.log(this.filterData);\n    },\n    onChange (dateString) {\n      let aa =  deepCopy(dateString[0])\n      let bb =  deepCopy(dateString[1])\n      console.log(aa.format('YYYY-MM-DD 00:00:00'));\n      console.log(bb.format('YYYY-MM-DD 23:59:59'));\n      this.filter[1].vModel = []\n      this.filter[1].vModel.push(aa.format('YYYY-MM-DD 00:00:00'))\n      this.filter[1].vModel.push(bb.format('YYYY-MM-DD 23:59:59'))\n      console.log(this.filter[1].vModel);\n    },\n    onOk (dateString) {\n      this.filter[1].vModel = dateString\n    },\n    log (childValue) {\n      console.log(childValue)\n    }\n  },\n  watch:{\n    $route: {\n      handler () {\n        this.switchTable()\n        // this.checkLogin()\n        this.refresh()\n      }\n    }\n  },\n  mounted () {\n    this.switchTable()\n    // let todayDate = new Date();\n    // const date = todayDate.toLocaleDateString().replace(/\\//g, '').replace(/-(\\d)\\b/g, '-0$1')\n    // this.selectDate = date\n    // this.checkLogin()\n  }\n}\n\n</script>\n\n<style>\n.tableButton {\n  float:right;\n  margin: 20px;\n  z-index: 100;\n}\n\n.SelectMonth {\n  position: absolute;\n  float: left;\n  margin: 20px;\n  z-index: 100;\n}\n</style>"]}]}