    if(this.$route.query.type=='page')\n        {return 1}\n      if(this.$route.query.type=='work')\n        {return 2}\n      if(this.$route.query.type=='skill')\n        {return 3}\n      else{return -1}\n    }\n  },\n  methods:{\n    refreshData(){\n    this.profileInfo = {}\n    request.request({\n      url:'http://39.107.227.252/getParagraphDetail/',\n      data:{'pid': this.pid,},\n      method: 'post',\n    }).then(res =>{\n      this.profileInfo = res.data\n    }).catch(err =>{\n      console.log(err);\n    })\n    }\n  },\n  watch:{\n    '$route': 'refreshData'\n  },\n  created() {\n    this.refreshData()\n  }\n}\n\n</script>\n<style>\n.partprofile{\n  min-height: 700px;\n}\n</style>"]}]}