equest from '../../network/request'\n\n\n\nexport default {\n  components: { Rightmsg, Row,Left, Mid, Studylist },\n    name: 'StudyList',\n    data(){\n      return{\n        type:this.$route.query.type,\n        foreword:'',\n        slist:[],\n      }\n    },\n    methods:{\n      refresh(){\n        let type = this.$route.query.type\n        request.request({\n        url:'http://39.107.227.252/getStudyType/',\n        data:{'sType': type,},\n        method: 'post',\n        }).then(res =>{\n          this.foreword = res.data.msg\n        }).catch(err =>{\n          console.log(err);\n        })\n        request.request({\n        url:'http://39.107.227.252/getStudyList/',\n        data:{'sType': type,},\n        method: 'post',\n        }).then(res =>{\n            this.slist = res.data.slist\n        }).catch(err =>{\n            console.log(err);\n        })\n      }\n    },\n    watch:{\n    '$route': 'refresh'\n    },\n    mounted(){\n      this.refresh()\n    }\n}\n\n</script>\n\n\n<style>\n.StudyList{\n  margin: 0;\n  padding: 0;\n}\n\n</style>\n"]}]}